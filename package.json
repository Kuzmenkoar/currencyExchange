{
  "name": "part1",
  "version": "1.0.0",
  "description": "part1",
  "main": "index.jsx",
  "scripts": {
    "dev": "webpack-dev-server --env.dev",
    "prod": "webpack --env.prod",
    "test": "jest --colors --verbose",
    "lint": "./node_modules/.bin/eslint --config ./.eslintrc.json ./src --ext .jsx, .js",
    "prettier": "prettier --write \"**/*.{js,jsx}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "jest --bail --findRelatedTests --colors --verbose",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".*": "babel-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "cobertura",
      "lcov"
    ],
    "coverageDirectory": "<rootDir>/target/coverage",
    "testEnvironment": "node",
    "collectCoverage": false,
    "setupFiles": [
      "<rootDir>/config/jest.js",
      "<rootDir>/config/enzyme.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/mocks/file.mock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/mocks/style.mock.js"
    }
  },
  "private": true,
  "license": "ISC",
  "dependencies": {
    "connected-react-router": "^4.5.0",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-select": "1.2.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-form": "^7.4.2",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "acorn": "^6.0.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.3",
    "node-sass": "^4.9.4",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
